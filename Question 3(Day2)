col1 <- c(1,2,3)
col2 <- c(4,5,6)
col3 <- c(7,8,9)
col4 <- c(10,11,12)
row1 <- c(1,2,3)
row2 <- c(4,5,6)
row3 <- c(7,8,9)
table1 <-array(c(col1,col2,col3),dim=c(3,3))
table2 <-array(c(col2,col3,col4),dim=c(3,3))
arr <-array(c(table1[1,],table2[1,],table1[2,],table2[2,],table1[3,],table2[3,]),dim=c(3,2,3))
print(arr)
